{
    "compilerOptions": {
        "target": "es2016", 
        "lib": ["ES6"],
        "module": "commonjs",
        "rootDir": "src",
        "outDir": "dist",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
    },
    "include": ["src"]
}
